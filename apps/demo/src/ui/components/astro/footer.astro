---
import type { Props } from '@astrojs/starlight/props';
import Default from '@astrojs/starlight/components/Footer.astro';
import { Icon } from '@astrojs/starlight/components';
---

<Default {...Astro.props}>
    <slot />
</Default>
<div class="mx-auto max-w-screen-xl px-4 pb-8 pt-16 sm:px-6 lg:px-8">
    <div class="mt-16 border-t border-gray-700 pt-8">
        <p class="text-center text-xs/relaxed text-gray-500">
            <Icon name="heart" color="pink" /><br />
            Crystallize Librairies are distributed under the MIT License.
        </p>
    </div>
</div>
